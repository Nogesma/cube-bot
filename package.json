    "codestyle": "prettier -c **/*.js *.json"
  },
  "dependencies": {
    "bluebird": "^3.7.2",
    "chalk": "^5.3.0",
    "cookie-parser": "^1.4.6",
    "cron": "^2.4.4",
    "dayjs": "^1.11.11",
    "discord.js": "^14.15.2",
    "express": "^4.19.2",
    "fs-extra": "^11.2.0",
    "mongoose": "^7.6.11",
    "nanoid": "^4.0.2",
    "node-fetch": "^3.3.2",
    "ramda": "^0.28.0",
    "scrambow": "^1.8.1",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.10",
    "debugging-aid": "^0.6.8",
    "dotenv": "^16.4.5",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5"
