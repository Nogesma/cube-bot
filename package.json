    "codestyle": "prettier -c **/*.js *.json"
  },
  "dependencies": {
    "bluebird": "^3.7.2",
    "chalk": "^5.6.2",
    "cookie-parser": "^1.4.7",
    "cron": "^4.3.4",
    "dayjs": "^1.11.19",
    "discord.js": "^14.25.1",
    "express": "^5.1.0",
    "fs-extra": "^11.3.2",
    "mongoose": "^8.20.1",
    "nanoid": "^5.1.6",
    "node-fetch": "^3.3.2",
    "ramda": "^0.31.3",
    "scrambow": "^1.8.1",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@types/express": "^5.0.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "debugging-aid": "^0.6.8",
    "dotenv": "^17.2.3",
    "jest": "^30.2.0",
    "nodemon": "^3.1.11",
    "prettier": "^3.7.1"
