    "codestyle": "prettier -c **/*.js *.json"
  },
  "dependencies": {
    "bluebird": "^3.7.2",
    "chalk": "^5.0.1",
    "cookie-parser": "^1.4.6",
    "cron": "^2.0.0",
    "dayjs": "^1.11.2",
    "discord.js": "^13.7.0",
    "express": "^4.18.1",
    "fs-extra": "^10.1.0",
    "mongoose": "^6.3.4",
    "nanoid": "^3.3.4",
    "node-fetch": "^3.2.4",
    "ramda": "^0.28.0",
    "scrambow": "^1.7.0",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.0",
    "@types/node": "^17.0.35",
    "debugging-aid": "^0.6.7",
    "dotenv": "^16.0.1",
    "jest": "^28.1.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2"
